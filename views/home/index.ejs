<div>
  <div class="main">
      <h1>Book Your Next Movie!</h1>
      <ul class="cards">
    <% movies.forEach(function(movie){ %>
        <li class="cards_item">
          <div class="card">
            <div class="card_image"><img src="https://1.bp.blogspot.com/-5KbTHuONOkw/VNyhBzgRh6I/AAAAAAAAC9M/yYaYz5VvmCE/s1600/Spirited_Away_Wallpaper_by_behruz.jpg"></div>
            <div class="card_content">
              <h2 class="card_title">  
                <!-- TITLE OF MOVIE   -->
              
                <%= movie.title %>
              </h2>

              <p class="card_text"></p>


              
              <a href="/movies/detail?id=<%=movie._id %>"><button class="btn card_btn">Book</button></a>

        
    



            </div>

            

          </div>
        </li>
        <%}) %>

      </ul>
    </div>

</div>

